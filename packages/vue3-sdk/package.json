{
  "name": "@bloomreach/vue3-sdk",
  "version": "22.0.0",
  "description": "Bloomreach SPA SDK for Vue 3",
  "keywords": [
    "bloomreach",
    "sdk",
    "vue"
  ],
  "author": "Bloomreach B.V.",
  "license": "Apache-2.0",
  "homepage": "https://www.bloomreach.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/bloomreach/spa-sdk",
    "directory": "packages/vue3-sdk"
  },
  "bugs": {
    "url": "https://issues.onehippo.com/projects/SPASDK/issues"
  },
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "typings.d.ts",
  "files": [
    "dist",
    "typings.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test": "vitest --watch=false",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "type-check:tests": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "type-check:watch": "npm run type-check -- --watch",
    "yalcAdd": "yalc add @bloomreach/spa-sdk",
    "yalcPush": "yalc push --replace --no-scripts",
    "yalcRetreat": "yalc retreat --all",
    "yalcRestore": "yalc restore --all"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/jsdom": "21.1.1",
    "@types/node": "18.16.14",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitejs/plugin-vue-jsx": "3.0.1",
    "@vue/test-utils": "2.4.0-alpha.1",
    "@vue/tsconfig": "0.1.3",
    "npm-run-all": "4.1.5",
    "typescript": "4.7.4",
    "vite": "4.3.8",
    "vitest": "0.29.8",
    "vue": "3.3.4",
    "vue-tsc": "1.6.5",
    "yalc": "1.0.0-pre.53"
  },
  "dependencies": {
    "@bloomreach/spa-sdk": "file:.yalc/@bloomreach/spa-sdk"
  }
}
