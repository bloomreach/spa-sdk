{
  "name": "@bloomreach/vue3-sdk",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
