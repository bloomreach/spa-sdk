{
  "name": "@bloomreach/example-react",
  "description": "Example React SPA for Bloomreach Experience",
  "version": "25.0.1",
  "private": true,
  "dependencies": {
    "@bloomreach/react-sdk": "25.0.1",
    "@bloomreach/spa-sdk": "25.0.1",
    "axios": "1.8.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "7.5.0",
    "sanitize-html": "2.14.0"
  },
  "scripts": {
    "build": "cross-env GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "lint": "eslint --ext js,ts,tsx src",
    "start": "serve build -sn -p 3002",
    "dev": "cross-env BROWSER=NONE PORT=3002 GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts start"
  },
  "eslintConfig": {
    "extends": "@bloomreach/eslint-config-react"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "7.25.9",
    "@bloomreach/eslint-config-react": "1.2.0",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@types/sanitize-html": "2.11.0",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "react-scripts": "5.0.1",
    "serve": "14.2.0",
    "typescript": "4.1.6"
  }
}
