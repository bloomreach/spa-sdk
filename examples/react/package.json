{
  "name": "@bloomreach/example-react",
  "description": "Example React SPA for Bloomreach Experience",
  "version": "23.5.0",
  "private": true,
  "dependencies": {
    "@bloomreach/react-sdk": "23.5.0",
    "@bloomreach/spa-sdk": "23.5.0",
    "axios": "1.6.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.2",
    "sanitize-html": "2.13.0"
  },
  "scripts": {
    "build": "cross-env GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "lint": "eslint --ext js,ts,tsx src",
    "start": "serve build -sn",
    "dev": "cross-env GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts start"
  },
  "eslintConfig": {
    "extends": "@bloomreach/eslint-config-react"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@bloomreach/eslint-config-react": "1.2.0",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/react-router-dom": "5.3.3",
    "@types/sanitize-html": "2.11.0",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "react-scripts": "5.0.1",
    "serve": "14.2.0",
    "typescript": "4.1.6"
  }
}
