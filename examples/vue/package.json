{
  "name": "@bloomreach/example-vue",
  "description": "Example Vue.js SPA for Bloomreach Experience",
  "version": "21.0.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "dev": "npm run serve",
    "lint": "vue-cli-service lint --no-fix",
    "serve": "vue-cli-service serve",
    "test": "vue-cli-service test:unit --pass-with-no-tests",
    "start": "serve dist -sn"
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version",
    "last 1 edge version"
  ],
  "dependencies": {
    "@bloomreach/spa-sdk": "21.0.0",
    "@bloomreach/vue-sdk": "21.0.0",
    "axios": "0.26.1",
    "core-js": "3.16.0",
    "sanitize-html": "2.10.0",
    "vue": "2.6.14",
    "vue-class-component": "7.2.6",
    "vue-property-decorator": "9.1.2",
    "vue-router": "3.5.2"
  },
  "devDependencies": {
    "@bloomreach/eslint-config-vue": "1.3.0",
    "@types/jest": "26.0.24",
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-eslint": "4.5.13",
    "@vue/cli-plugin-router": "4.5.13",
    "@vue/cli-plugin-typescript": "4.5.13",
    "@vue/cli-plugin-unit-jest": "4.5.13",
    "@vue/cli-service": "4.5.13",
    "@vue/eslint-config-airbnb": "5.3.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vue/test-utils": "1.2.2",
    "css-loader": "5.2.7",
    "eslint": "7.32.0",
    "postcss-loader": "4.3.0",
    "sass-loader": "10.4.1",
    "serve": "14.2.0",
    "typescript": "4.4.4",
    "vue-server-renderer": "2.6.14",
    "vue-template-compiler": "2.6.14",
    "webpack": "4.46.0"
  },
  "overrides": {
    "eslint": "7.32.0",
    "eslint-plugin-vue": "8.7.1"
  },
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel"
  },
  "eslintConfig": {
    "extends": [
      "@bloomreach/eslint-config-vue"
    ],
    "rules": {
      "max-len": "off",
      "object-curly-newline": "off"
    }
  }
}
