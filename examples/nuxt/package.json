{
  "name": "@bloomreach/example-nuxt",
  "description": "Example Nuxt SPA for Bloomreach Experience",
  "version": "23.3.2",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxt prepare",
    "yalcAdd": "yalc add @bloomreach/spa-sdk @bloomreach/vue-sdk",
    "yalcRetreat": "yalc retreat --all",
    "yalcRestore": "yalc restore --all"
  },
  "dependencies": {
    "@bloomreach/spa-sdk": "file:.yalc/@bloomreach/spa-sdk",
    "@bloomreach/vue-sdk": "file:.yalc/@bloomreach/vue-sdk",
    "axios": "1.6.8",
    "nuxt": "3.11.2",
    "sanitize-html": "2.10.0",
    "vue": "3.4.26",
    "vue-router": "4.3.2"
  },
  "devDependencies": {
    "yalc": "1.0.0-pre.53"
  }
}
