{
  "name": "@bloomreach/example-nuxt",
  "version": "19.0.1",
  "description": "Example Nuxt.js SPA for Bloomreach Experience",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue .",
    "start": "cross-env NODE_ENV=production node server/index.js"
  },
  "dependencies": {
    "@bloomreach/segmentation": "0.0.13",
    "@bloomreach/spa-sdk": "19.0.1",
    "@bloomreach/vue-sdk": "19.0.1",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/dotenv": "1.4.1",
    "consola": "2.15.3",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "nuxt": "2.15.7",
    "nuxt-property-decorator": "2.9.1"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "7.14.9",
    "@bloomreach/eslint-config-nuxt": "1.0.1",
    "@nuxt/types": "2.15.7",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "babel-eslint": "10.1.0",
    "core-js": "3.16.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "nodemon": "2.0.12",
    "prettier": "2.3.2",
    "ts-loader": "^8.0.17"
  },
  "eslintConfig": {
    "extends": [
      "@bloomreach/eslint-config-nuxt"
    ],
    "rules": {
      "vue/html-self-closing": "off",
      "vue/singleline-html-element-content-newline": "off"
    }
  }
}
