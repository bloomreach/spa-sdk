{
  "name": "@bloomreach/example-nuxt",
  "version": "22.0.0",
  "description": "Example Nuxt.js SPA for Bloomreach Experience",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "generate": "nuxt generate",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "yalcAdd": "yalc add @bloomreach/spa-sdk @bloomreach/vue-sdk",
    "yalcRetreat": "yalc retreat --all",
    "yalcRestore": "yalc restore --all"
  },
  "dependencies": {
    "@bloomreach/spa-sdk": "file:.yalc/@bloomreach/spa-sdk",
    "@bloomreach/vue-sdk": "file:.yalc/@bloomreach/vue-sdk",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/dotenv": "1.4.1",
    "consola": "2.15.3",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "nuxt": "2.15.7",
    "nuxt-property-decorator": "2.9.1",
    "sanitize-html": "2.10.0",
    "vue": "2.6.14"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "7.14.9",
    "@nuxt/types": "2.15.7",
    "@nuxt/typescript-build": "2.1.0",
    "core-js": "3.16.0",
    "nodemon": "2.0.12",
    "ts-loader": "8.4.0",
    "vue-server-renderer": "2.6.14",
    "vue-template-compiler": "2.6.14",
    "yalc": "1.0.0-pre.53"
  }
}
